// Test in function
def validateEmail(email) {
  pattern = /^[^\s@]+@[^\s@]+\.[^\s@]+$/
  return pattern.test(email)
}

email1 = validateEmail('test@example.com')
email2 = validateEmail('invalid.email')

// Regex with special chars
