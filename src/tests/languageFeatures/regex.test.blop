// Regex with object destructuring
def parseUrl(url) {
  pattern = /^(https?):\/\/([^\/]+)(\/.*)?$/
  matches = url.match(pattern)
  if matches {
    return {
      protocol: matches[1],
      domain: matches[2],
      path: matches[3]
    }
  }
  return undefined
}

urlParts = parseUrl('https://example.com/path/to/page')

