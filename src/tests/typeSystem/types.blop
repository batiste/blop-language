
type User = {
    name: string,
    id: number,
    userType?: "Admin" | "User"
}

type Post = {
    title: string,
    author: string,
    views: number
}

type Profile = {
    user: User,
    bio?: string,
    verified: boolean
}

type choices = "option1" | "option2" | "option3"

def setUser(user: User) {
    user.userType = "User"
}

def profile(prof: Profile) {
    setUser(prof.user)
}

