
def render(text) {
  <h1>text</h1>
}

// should produce no warning
def testConditions() {
  if true {
    if false {
      <div>1</div>
    } elseif 1 {
      <div>2</div>
    } else {
      <div>3</div>
    }
  } elseif 1 {
    <div>4</div>
  } else {
    <div>5</div>
  }
}

test('virtual node', () => {
  test = render('hello')
  expect(test.children[0].text).toBe('hello')
  condition = testConditions()
  expect(condition.children[0].text).toBe(2)
})