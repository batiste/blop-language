
type User = {
    name: string,
    id: number,
    userType?: "Admin" | "User"
}

type Post = {
    title: string,
    author: string,
    views: number
}

type Profile = {
    user: User,
    bio?: string,
    verified: boolean
}

type choices = "option1" | "option2" | "option3"


def user(u: User) {
    u.userType = "User"
}

def profile(p: Profile) {
    user(p.user)
}

