{
  "name": "gg-parser",
  "version": "1.0.0",
  "description": "A simple parser",
  "main": "index.js",
  "scripts": {
    "start": "node src/generateParser && webpack-dev-server",
    "linter": "cp {src/parser.js,src/grammar.js,src/tokensDefinition.js} vscode/blop-linter/server/src/ && cd vscode/blop-linter && npm run compile",
    "link-extensions": "`ln -s $PWD/vscode/blop-linter ~/.vscode/extensions/blop-linter && ln -s $PWD/vscode/blop-syntax-highlighter ~/.vscode/extensions/blop-syntax-highlighter`"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/batiste/js-grammar-parser.git"
  },
  "keywords": [
    "parser",
    "grammar",
    "language"
  ],
  "author": "Batiste Bieler",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/batiste/js-grammar-parser/issues"
  },
  "devDependencies": {
    "mocha": "^2.2.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.10"
  },
  "homepage": "https://github.com/batiste/js-grammar-parser",
  "dependencies": {
    "snabbdom": "^0.7.2",
    "webpack": "^4.16.5"
  }
}
