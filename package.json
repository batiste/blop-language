{
  "name": "blop-language",
  "version": "0.9.3",
  "description": "Blop language is JavaScript-like language for the web",
  "main": "src/index.js",
  "loader": "src/loader.js",
  "jest-transform": "src/jest.js",
  "scripts": {
    "test": "jest",
    "parser": "node src/generateParser",
    "start": "npm run webpack-watch & npm run server",
    "server": "node_modules/nodemon/bin/nodemon.js dist/server.js",
    "webpack": "node_modules/webpack/bin/webpack.js",
    "webpack-watch": "node_modules/webpack/bin/webpack.js --watch",
    "debug": "BLOP_DEBUG=true node_modules/webpack/bin/webpack.js && node dist/server.js",
    "linter": "cp {src/parser.js,src/grammar.js,src/tokensDefinition.js,src/backend.js,src/builtin.js} vscode/blop-linter/server/src/ && cd vscode/blop-linter && npm run compile",
    "link-extensions": "`ln -s $PWD/vscode/blop-linter ~/.vscode/extensions/blop-linter && ln -s $PWD/vscode/blop-syntax-highlighter ~/.vscode/extensions/blop-syntax-highlighter`",
    "build": "npm run parser && npm run test && npm run linter && npm run webpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/batiste/blop-language.git"
  },
  "keywords": [
    "parser",
    "grammar",
    "language"
  ],
  "author": "Batiste Bieler",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/batiste/blop-language/issues"
  },
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-ie11": "^1.0.0",
    "eslint-plugin-import": "^2.14.0",
    "express": "^4.16.4",
    "jest": "^23.6.0",
    "node-fetch": "^2.3.0",
    "snabbdom-to-html": "^5.1.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2",
    "nodemon": "^1.18.9"
  },
  "homepage": "https://github.com/batiste/blop-language",
  "dependencies": {
    "loader-utils": "^1.1.0",
    "schema-utils": "^1.0.0",
    "snabbdom": "^0.7.3"
  }
}
